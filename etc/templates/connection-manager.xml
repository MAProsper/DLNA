<?xml version="1.0" encoding="UTF-8"?>
<srv:scpd xmlns:srv="urn:schemas-upnp-org:service-1-0">
	<srv:specVersion>
		<srv:major>1</srv:major>
		<srv:minor>0</srv:minor>
	</srv:specVersion>
	<srv:actionList>
