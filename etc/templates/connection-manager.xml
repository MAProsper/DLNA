					<srv:direction>out</srv:direction>
					<srv:relatedStateVariable>SourceProtocolInfo</srv:relatedStateVariable>
				</srv:argument>
				<srv:argument>
					<srv:name>Sink</srv:name>
					<srv:direction>out</srv:direction>
					<srv:relatedStateVariable>SinkProtocolInfo</srv:relatedStateVariable>
				</srv:argument>
			</srv:argumentList>
		</srv:action>
	</srv:actionList>
	<srv:serviceStateTable>
		<srv:stateVariable srv:sendEvents="no">
			<srv:name>SourceProtocolInfo</srv:name>
			<srv:dataType>string</srv:dataType>
		</srv:stateVariable>
		<srv:stateVariable srv:sendEvents="no">
			<srv:name>SinkProtocolInfo</srv:name>
			<srv:dataType>string</srv:dataType>
