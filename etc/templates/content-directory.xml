				<srv:argument>
					<srv:name>BrowseFlag</srv:name>
					<srv:direction>in</srv:direction>
					<srv:relatedStateVariable>A_ARG_TYPE_BrowseFlag</srv:relatedStateVariable>
				</srv:argument>
				<srv:argument>
					<srv:name>Filter</srv:name>
					<srv:direction>in</srv:direction>
					<srv:relatedStateVariable>A_ARG_TYPE_Filter</srv:relatedStateVariable>
				</srv:argument>
				<srv:argument>
					<srv:name>StartingIndex</srv:name>
					<srv:direction>in</srv:direction>
					<srv:relatedStateVariable>A_ARG_TYPE_Index</srv:relatedStateVariable>
				</srv:argument>
				<srv:argument>
					<srv:name>RequestedCount</srv:name>
					<srv:direction>in</srv:direction>
					<srv:relatedStateVariable>A_ARG_TYPE_Count</srv:relatedStateVariable>
				</srv:argument>
				<srv:argument>
					<srv:name>SortCriteria</srv:name>
					<srv:direction>in</srv:direction>
					<srv:relatedStateVariable>A_ARG_TYPE_SortCriteria</srv:relatedStateVariable>
