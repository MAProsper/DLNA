					<srv:name>NumberReturned</srv:name>
					<srv:direction>out</srv:direction>
					<srv:relatedStateVariable>A_ARG_TYPE_Count</srv:relatedStateVariable>
				</srv:argument>
				<srv:argument>
					<srv:name>TotalMatches</srv:name>
					<srv:direction>out</srv:direction>
					<srv:relatedStateVariable>A_ARG_TYPE_Count</srv:relatedStateVariable>
				</srv:argument>
				<srv:argument>
					<srv:name>UpdateID</srv:name>
					<srv:direction>out</srv:direction>
					<srv:relatedStateVariable>A_ARG_TYPE_UpdateID</srv:relatedStateVariable>
				</srv:argument>
			</srv:argumentList>
		</srv:action>
	</srv:actionList>
	<srv:serviceStateTable>
		<srv:stateVariable srv:sendEvents="no">
			<srv:name>A_ARG_TYPE_ObjectID</srv:name>
			<srv:dataType>string</srv:dataType>
		</srv:stateVariable>
		<srv:stateVariable srv:sendEvents="no">
			<srv:name>A_ARG_TYPE_Result</srv:name>
			<srv:dataType>string</srv:dataType>
		</srv:stateVariable>
		<srv:stateVariable srv:sendEvents="no">
			<srv:name>A_ARG_TYPE_SearchCriteria</srv:name>
			<srv:dataType>string</srv:dataType>
		</srv:stateVariable>
		<srv:stateVariable srv:sendEvents="no">
			<srv:name>A_ARG_TYPE_BrowseFlag</srv:name>
			<srv:dataType>string</srv:dataType>
			<srv:allowedValueList>
				<srv:allowedValue>BrowseDirectChildren</srv:allowedValue>
			</srv:allowedValueList>
		</srv:stateVariable>
		<srv:stateVariable srv:sendEvents="no">
